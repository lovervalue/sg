<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:idubbo="http://code.alibabatech.com/schema/idubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://code.alibabatech.com/schema/idubbo
	http://code.alibabatech.com/schema/idubbo/idubbo.xsd"
	default-lazy-init="false">

	<idubbo:service interface="org.activiti.engine.RepositoryService" ref="repositoryService" />
	<idubbo:service interface="org.activiti.engine.RuntimeService" ref="runtimeService"/>
	<idubbo:service interface="org.activiti.engine.FormService" ref="formService"/>
	<idubbo:service interface="org.activiti.engine.IdentityService" ref="identityService"/>
	<idubbo:service interface="org.activiti.engine.TaskService" ref="taskService"/>
	<idubbo:service interface="org.activiti.engine.HistoryService" ref="historyService"/>
	<idubbo:service interface="org.activiti.engine.ManagementService" ref="managementService"/>
</beans>