<TableDataMap>
<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT 地区||'省' as 地区,count(人员姓名) as 人数 from S人员花名册
group by 地区]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="ds2" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="bian"/>
<O>
<![CDATA[最高学历]]></O>
</Parameter>
<Parameter>
<Attributes name="di"/>
<O>
<![CDATA[河南]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select 地区,${bian} as 参照,count(人员姓名) as 人数
from S人员花名册
where 地区='${di}'
group by 地区,${bian} ]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="ds3" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select '发放进度' as 工资类型, round(sum(累计实发/本年工资预算)*100,2) as 工资
from 工资预算与实际发放
union
select '平均工资' as 工资类型,round(sum(累计实发)/(select count(人员姓名) from S人员花名册),2) as 工资
from 工资预算与实际发放]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
<TableData name="ds4" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[FRDemo]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT '岗位工资' as 项目,sum(岗位工资) as 金额
FROM 公司工资总额统计
union
select '绩效工资' as 项目,sum(绩效工资) as 金额
FROM 公司工资总额统计
union
select '加班工资' as 项目,sum(加班工资) as 金额
FROM 公司工资总额统计
union
select '津贴补贴' as 项目,sum(津贴补贴) as 金额
FROM 公司工资总额统计
union
select '其他工资' as 项目,sum(其他工资) as 金额
FROM 公司工资总额统计
union
select '生活费' as 项目,sum(生活费) as 金额
FROM 公司工资总额统计]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
